<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.projectfour_musicalstructureapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_headset_white_24dp"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:icon="@drawable/ic_headset_white_24dp">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Stream"
            android:label="@string/stream_activity"/>
        <activity android:name=".SongListActivity"
            android:label="@string/song_list_activity"/>
        <activity android:name=".SongActivity"
            android:label="@string/song_activity"/>
        <activity android:name=".PlaylistActivity"
            android:label="@string/playlist_activity"/>
    </application>

</manifest>